
int main() {
    int ret = 0;

    int _int;
    double _double;
    string _string;
    bool _bool;

    if (_int != 0)
        error("err");
    else
        if (_double != 0.0)
            error("err");
        else
            if (_string != "")
                error("err");
            else
                if (_bool)
                    error("err");

    printInt(ret);
    return ret;
}
