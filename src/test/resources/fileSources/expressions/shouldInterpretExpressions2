
int main() {
    int ret = 0;

    int exp1 = -1 - 5;
    printInt(exp1);

    bool exp2 = -1 - 5 == -1 + (-5);
    if (exp2)
        printInt(1);
    else
        printInt(-1);

    bool exp3 = -1 - 5 == -6;
    if (exp3)
        printInt(1);
    else
        printInt(-1);


    int exp4 = 1 + -5;
    printInt(exp4);


    int exp5 = 1 % 3;
    printInt(exp5);

    int exp6 = 2 * 1 % 3;
    printInt(exp6);

    int exp7 = 2 % 3 / 3;
    printInt(exp7);

    printInt(ret);
    return 0;
}
