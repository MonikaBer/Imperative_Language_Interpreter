
int main() {
    int ret = 0;

    int exp1 = -1 - 5;
    printInt(exp1);

    bool exp2 = -1 - 5 == -1 + (-5);
    if (exp2)
        printInt(1);
    else
        printInt(-1);

    bool exp3 = -1 - 5 == -6;
    if (exp3)
        printInt(1);
    else
        printInt(-1);


    int exp4 = 1 + -5;
    printInt(-4);

    printInt(ret);
    return 0;
}
